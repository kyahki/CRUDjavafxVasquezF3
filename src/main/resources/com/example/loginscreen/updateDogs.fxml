<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.loginscreen.HelloController">
    <children>
        <VBox fx:id="pnUpdateDog" alignment="CENTER" prefHeight="359.0" prefWidth="339.0" spacing="20.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
            <Label text="Update Dog!">
                <font>
                    <Font size="23.0" />
                </font>
            </Label>
            <GridPane prefHeight="265.0" prefWidth="298.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="147.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="171.0" minWidth="10.0" prefWidth="152.0" />
                    <ColumnConstraints />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <children>
                    <Label text="Dog ID:" GridPane.rowIndex="0" />
                    <TextField fx:id="txtDogID" GridPane.columnIndex="1" />
                    <Label text="New Dog Name:" GridPane.rowIndex="1" />
                    <TextField fx:id="txtNewDogName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    <Button alignment="CENTER" mnemonicParsing="false" onAction="#onUpdateDogName" prefHeight="25.0" prefWidth="131.0" text="Update Dog Name" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
                    <Label text="New Dog Age:" GridPane.rowIndex="3" />
                    <TextField fx:id="txtNewDogAge" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                    <Button alignment="CENTER" mnemonicParsing="false" onAction="#onUpdateDogAge" prefHeight="25.0" prefWidth="131.0" text="Update Dog Age" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="4" />
                    <Label fx:id="lblUpdate" prefHeight="40.0" prefWidth="147.0" GridPane.rowIndex="5">
                        <font>
                            <Font size="8.0" />
                        </font></Label>
                    <Button mnemonicParsing="false" onAction="#onGoBack4" text="Go Back" GridPane.rowIndex="6" />
                </children>
            </GridPane>
        </VBox>
    </children>
</AnchorPane>
